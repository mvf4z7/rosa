{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "Boilerplate for ReactJS project with hot code reloading and an ExpressJS server",
  "scripts": {
    "start": "NODE_ENV=production PORT=8001 nodejs bin/www",
    "start-dev": "nodemon bin/www",
    "start-hot": "node hot-server.js",
    "build": "webpack",
    "lint": "node_modules/eslint/bin/eslint.js src/*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gaearon/react-hot-boilerplate.git"
  },
  "devDependencies": {
    "babel-core": "^5.4.7",
    "babel-eslint": "^3.1.9",
    "babel-loader": "^5.1.2",
    "css-loader": "^0.15.6",
    "eslint-plugin-react": "^2.3.0",
    "node-sass": "^3.2.0",
    "react-hot-loader": "^1.2.7",
    "sass-loader": "^1.0.4",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.6",
    "webpack-dev-server": "^1.8.2"
  },
  "dependencies": {
    "alt": "^0.17.3",
    "body-parser": "^1.13.3",
    "chart.js": "^1.0.2",
    "cookie-parser": "^1.3.5",
    "debug": "^2.2.0",
    "express": "^4.13.2",
    "material-ui": "^0.11.0",
    "morgan": "^1.6.1",
    "proxy-middleware": "^0.13.0",
    "react": "^0.13.3",
    "react-highcharts": "^2.0.0",
    "react-router": "^0.13.3",
    "react-tap-event-plugin": "^0.1.7",
    "socket.io": "^1.3.6",
    "socket.io-client": "^1.3.6",
    "superagent": "^1.3.0"
  }
